<odoo>

    <record model="ir.ui.view" id="res_partner_form_view">
        <field name="name">res.partner.form (in account_credit_control_analysis)</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="account_credit_control.partner_followup_form_view"/>
        <field name="arch" type="xml">
            <field name="payment_next_action_date" position="after">
                <field name="credit_control_analysis_ids" nolabel="1" colspan="2">
                    <tree editable="bottom">
                        <field name="policy_id"/>
                        <field name="policy_level_id"/>
                        <field name="level"/>
                    </tree>
                </field>
            </field>
        </field>
    </record>



</odoo>
